<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.AdminMapper">
    <select id="adminLogin" resultType="bean.AdminInfo" parameterType="bean.AdminInfo">
        select acc,pwd from admin where acc=#{acc} and pwd=#{pwd}
    </select>


</mapper>